@model Models.Ware

@{
    ViewData["Title"] = "Delete Ware";
}

<h1>Delete Ware</h1>

<h3>Are you sure you want to delete this?</h3>
<div>
    <h4>Ware</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Name
        </dt>
        <dd class="col-sm-10">
            @Model.Name
        </dd>
        <dt class="col-sm-2">
            Value
        </dt>
        <dd class="col-sm-10">
            @Model.Value
        </dd>
        <dt class="col-sm-2">
            Property
        </dt>
        <dd class="col-sm-10">
            @Model.Property
        </dd>
    </dl>
    
    <form asp-action="DeleteConfirmed">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Index">Back to List</a>
    </form>
</div>
